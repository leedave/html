{
    "name": "leedch/html",
    "type": "library",
    "description": "Module for generating HTML in PHP",
    "keywords": [
        "leedch",
        "html",
        "html5",
        "generate"
    ],
    "homepage": "https://github.com/leedave/html.git",
    "license": "MIT",
    "authors": [
        {
            "name": "David Lee",
            "email": "leed@leed.ch",
            "homepage": "https://www.leed.ch/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Leedch\\Html\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leedch\\Html\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}